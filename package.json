{
  "name": "sencrop-developer-platform",
  "private": true,
  "description": "The Sencrop Developer Platform website",
  "version": "2.0.0",
  "author": "Paul Bottein <paul.bottein@gmail.com>",
  "contributors": [
    "Frédéric Espiau <frederic.espiau@gmail.com>"
  ],
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sencrop/sencrop-developer-platform"
  },
  "bugs": {
    "url": "https://github.com/sencrop/sencrop-developer-platform"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint 'src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.js'",
    "start": "npm run develop",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@sencrop/ui-components": "^8.7.0",
    "assert": "^2.0.0",
    "formik": "^2.2.9",
    "gatsby": "^3.14.2",
    "gatsby-background-image": "^1.5.3",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-netlify": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-react-leaflet": "^3.0.2",
    "gatsby-plugin-sharp": "^3.14.1",
    "gatsby-plugin-styled-components": "^4.14.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-autolink-headers": "^4.11.0",
    "gatsby-remark-external-links": "~0.0.4",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.5.1",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "path-browserify": "^1.0.1",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-leaflet": "^3.2.2",
    "react-leaflet-markercluster": "^3.0.0-rc1",
    "shortid": "^2.2.16",
    "slugify": "^1.6.1",
    "styled-components": "^5.3.1",
    "styled-media-query": "^2.1.2",
    "swagger-ui": "^3.52.4",
    "swagger-ui-dist": "^3.52.4",
    "webfontloader": "^1.6.28",
    "yup": "~0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "prettier": "2.4.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn",
      "react/prop-types": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  },
  "volta": {
    "node": "14.18.1",
    "npm": "8.0.0"
  }
}
