{
  "name": "sencrop-developer-platform",
  "private": true,
  "description": "The Sencrop Developer Platform website",
  "version": "2.0.0",
  "author": "Paul Bottein <paul.bottein@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sencrop/sencrop-developer-platform"
  },
  "bugs": {
    "url": "https://github.com/sencrop/sencrop-developer-platform"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint 'src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.js'",
    "start": "npm run develop"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@sencrop/ui-components": "^4.6.1",
    "formik": "^2.2.9",
    "gatsby": "^2.32.13",
    "gatsby-background-image": "^1.5.3",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-catch-links": "^2.10.0",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-mdx": "^1.10.1",
    "gatsby-plugin-netlify": "^2.11.1",
    "gatsby-plugin-offline": "^3.10.2",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-react-leaflet": "^2.0.13",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-external-links": "~0.0.4",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-remark": "^2.16.1",
    "gatsby-transformer-sharp": "^2.12.1",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.28.0",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-animate-height": "^2.0.23",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-leaflet": "^2.8.0",
    "react-leaflet-markercluster": "^2.0.0",
    "shortid": "^2.2.16",
    "slugify": "^1.6.1",
    "styled-components": "^5.3.1",
    "styled-media-query": "^2.1.2",
    "swagger-ui": "^3.52.4",
    "swagger-ui-dist": "^3.52.4",
    "webfontloader": "^1.6.28",
    "yup": "~0.29.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^4.3.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.1.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn",
      "react/prop-types": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "volta": {
    "node": "14.18.1",
    "npm": "8.0.0"
  }
}
